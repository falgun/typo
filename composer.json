{
    "name": "falgunphp/typo",
    "type": "library",
    "homepage": "https://github.com/falgun/typo",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "S.M. Ataur Rahman",
            "email": "dev.ataur.rahman@gmail.com",
            "homepage": "https://ataur.net/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Falgun\\Typo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Falgun\\Typo\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0",
        "falgunphp/kuery": "dev-master",
        "falgunphp/typo-generator": "dev-main"
    },
    "require-dev": {
        "phpro/grumphp": "^1.3",
        "phpunit/phpunit": "^9.5",
        "sempro/phpunit-pretty-print": "^1.4",
        "squizlabs/php_codesniffer": "dev-master",
        "vimeo/psalm": "^4.7"
    },
    "scripts": {
        "phpunit": "phpunit --verbose --colors=always"
    },
    "config": {
        "sort-packages": true
    }
}
